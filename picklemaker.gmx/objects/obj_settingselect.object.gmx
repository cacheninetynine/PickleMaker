{
    sel += 1
    keyboard_string = ""
}
sel = clamp(sel,0,array_length_1d(options)-1)
if sel == 4
{
    if keyboard_check_pressed(vk_left)
    {
        sel2 -= 1
    }
    if keyboard_check_pressed(vk_right)
    {
        sel2 += 1
    }
    sel2 = clamp(sel2,0,array_length_1d(games)-1)
}
keyboard_string = string_copy(keyboard_string, 1, 32);
if keyboard_check_pressed(vk_enter)
{
    switch sel
    {
        case 0:
            global.levelname = keyboard_string
            instance_destroy()
            break
        
        case 1:
            global.levelauthor = keyboard_string
            instance_destroy()
            break
            
        case 2:
            room_width = real(string_digits(keyboard_string))
            instance_destroy()
            break
            
        case 3:
            room_height = real(string_digits(keyboard_string))
            instance_destroy()
            break
            
        case 4:
            instance_destroy()
            scr_setgame(real(sel2))
            break
        
        case 5:
            if keyboard_string == "YES"
                game_restart()
            else
                instance_destroy()
            break
    }
}
</string>
